{
  "name": "@glue-template/iac",
  "version": "0.1.0",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "vitest run",
    "check": "run-p check:*",
    "check:format": "npx @biomejs/biome format .",
    "check:lint": "npx @biomejs/biome lint .",
    "check:type": "tsc --noEmit",
    "check:spell": "cspell lint . --cache --gitignore",
    "fix": "npx @biomejs/biome format . --write"
  },
  "devDependencies": {
    "aws-cdk": "2.127.0",
    "vitest": "1.3.0"
  },
  "dependencies": {
    "aws-cdk-lib": "2.127.0",
    "constructs": "10.3.0",
    "esbuild": "0.20.0",
    "source-map-support": "0.5.21"
  }
}
